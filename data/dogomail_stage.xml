<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record model="sale.dogomail.domain.stage" id="draft_stage">
            <field name="name">Draft</field>
            <field name="sequence">5</field>
            <field name="category">draft</field>
        </record>

        <record model="sale.dogomail.domain.stage" id="active_stage">
            <field name="name">Active</field>
            <field name="sequence">10</field>
            <field name="category">active</field>
        </record>

        <record model="sale.dogomail.domain.stage" id="done_stage">
            <field name="name">Done</field>
            <field name="sequence">15</field>
            <field name="fold" eval="True"/>
            <field name="category">done</field>
        </record>

    </data>
</odoo>
